alias dtest='poetry run python manage.py test --keepdb'
alias dbtest='poetry run python manage.py test'
alias dshell='poetry run python manage.py shell'
alias 'gitforce'='git push && git pull'
alias 'start'='poetry run python manage.py runserver'
alias 'migrate'='poetry run python manage.py migrate'
alias 'mmigrate'='poetry run python manage.py makemigrations'
alias 'init-migrate'='python manage.py migrate --fake'
alias 'envupdate'='poetry install'
